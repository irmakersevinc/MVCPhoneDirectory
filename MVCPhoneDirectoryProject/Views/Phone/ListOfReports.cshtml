@model IEnumerable<MVCPhoneDirectoryProject.Models.ReportModel>


@{
    ViewBag.Title = "ListOfReports";
}

<h2>List Of Reports</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UUID)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfPeople)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UUID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfPeople)
            </td>


        </tr>
    }


</table>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
